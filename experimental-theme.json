					},
					{
						"slug": "secondary",
						"name": "Secondary",
						"color": "#FAFBF6"
					}
				]
			},
			"typography": {
				"customLineHeight": true,
				"fontFamilies": [
					{
						"fontFamily": "\"Manrope\", Helvetica, Arial, sans-serif",
